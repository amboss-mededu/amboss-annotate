{
  "name": "@amboss-mededu/amboss-annotate",
  "version": "0.0.40",
  "description": "DOM annotation",
  "author": "Alex Jeffcott",
  "license": "ISC",
  "lockfileVerion": 1,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amboss-mededu/amboss-annotate.git"
  },
  "bugs": {
    "url": "https://github.com/amboss-mededu/amboss-annotate/issues"
  },
  "homepage": "https://github.com/amboss-mededu/amboss-annotate#readme",
  "files": [
    "dist"
  ],
  "main": "./dist/amboss-annotate.umd.js",
  "module": "./dist/amboss-annotate.es.js",
  "exports": {
    ".": {
      "import": "./dist/amboss-annotate.es.js",
      "require": "./dist/amboss-annotate.umd.js"
    }
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "clean": "rm -rf ./src/phrasios_de.json ./src/phrasios_us.json ./src/terms_de_de.json ./src/terms_us_en.json",
    "createTermToIdMap": "node createTermToIdMap.mjs",
    "createIdToPhrasioMap": "node createIdToPhrasioMap.mjs",
    "createIdToPhrasioMap:empty": "node createIdToPhrasioMapEmpty.mjs",
    "linkLocalAmbossPhrasio": "ln -sF /Users/ajt/projects/amboss-phrasio/dist /Users/ajt/projects/amboss-annotate/node_modules/@amboss-mededu/amboss-phrasio/dist",
    "build": "npm run clean && npm run createTermToIdMap && npm run createIdToPhrasioMap:empty && vite build",
    "build:offline": "npm run clean && npm run createTermToIdMap && npm run createIdToPhrasioMap && vite build",
    "build:local": "vite build",
    "run:local": "npm run linkLocalAmbossPhrasio && vite",
    "cy": "cypress open",
    "legacy": "vite build --config vite.config.legacy.ts"
  },
  "devDependencies": {
    "@types/node": "^17.0.14",
    "@vitejs/plugin-legacy": "^1.6.4",
    "cypress": "^9.4.1",
    "eslint": "^8.8.0",
    "http-server": "^14.1.0",
    "node-fetch": "^3.2.0",
    "regenerator-runtime": "^0.13.9",
    "typescript": "^4.5.5",
    "vite": "^2.7.13"
  },
  "dependencies": {
    "@amboss-mededu/amboss-phrasio": "^0.0.51",
    "@popperjs/core": "^2.11.2",
    "lodash.throttle": "^4.1.1"
  }
}
