{
  "name": "@amboss-mededu/amboss-annotate",
  "version": "0.0.45",
  "description": "DOM annotation",
  "author": "Alex Jeffcott",
  "license": "MIT",
  "lockfileVerion": 1,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amboss-mededu/amboss-annotate.git"
  },
  "bugs": {
    "url": "https://github.com/amboss-mededu/amboss-annotate/issues"
  },
  "homepage": "https://github.com/amboss-mededu/amboss-annotate#readme",
  "files": [
    "dist"
  ],
  "module": "./dist/amboss-annotate.es.js",
  "exports": {
    ".": {
      "import": "./dist/amboss-annotate.es.js"
    }
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "clean": "rm -rf dist ./src/phrasios_de.json ./src/phrasios_us.json ./src/terms_de_de.json ./src/terms_us_en.json",
    "createTermToIdMap": "node createTermToIdMap.mjs",
    "createIdToPhrasioMap": "node createIdToPhrasioMap.mjs",
    "createIdToPhrasioMap:empty": "node createIdToPhrasioMapEmpty.mjs",
    "linkLocalAmbossPhrasio": "ln -sF /Users/ajt/projects/amboss-phrasio/dist /Users/ajt/projects/amboss-annotate/node_modules/@amboss-mededu/amboss-phrasio",
    "linkLocalAdorn": "ln -sF /Users/ajt/projects/adorn/packages/annotate/build /Users/ajt/projects/amboss-annotate/node_modules/@fairfox/adorn",
    "build": "npm run clean && npm run createTermToIdMap && npm run createIdToPhrasioMap && vite build",
    "build:local": "vite build",
    "run:local": "vite"
  },
  "devDependencies": {
    "@types/node": "^17.0.33",
    "eslint": "^8.15.0",
    "http-server": "^14.1.0",
    "node-fetch": "^3.2.4",
    "typescript": "^4.6.4",
    "vite": "^2.9.9"
  },
  "dependencies": {
    "@amboss-mededu/amboss-phrasio": "^0.0.59",
    "@fairfox/adorn": "^1.0.23",
    "@popperjs/core": "^2.11.5",
    "@webcomponents/webcomponentsjs": "^2.6.0"
  }
}
